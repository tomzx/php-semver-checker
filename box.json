{
    "alias": "php-semver-checker.phar",
    "compactors": ["KevinGH\\Box\\Compactor\\Php"],
    "compression": "BZ2",
    "directories": ["src"],
    "files": ["LICENSE"],
    "finder": [
        {
            "name": "*.php",
            "exclude": ["test", "tests", "Tests"],
            "in": "vendor"
        }
    ],
    "git-tag": "package_version",
    "main": "bin/php-semver-checker",
    "output": "php-semver-checker.phar",
    "stub": true
}
